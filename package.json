{
  "name": "n8n-nodes-datadog",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Datadog providing 12 resources and 100+ operations for complete observability workflow automation including metrics, monitors, dashboards, events, logs, APM, synthetics, and more.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-datadog.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-datadog#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-datadog/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "datadog",
    "monitoring",
    "observability",
    "metrics",
    "apm",
    "logs",
    "dashboards",
    "alerts",
    "synthetics",
    "incidents",
    "slo",
    "devops",
    "automation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DatadogApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Datadog/Datadog.node.js",
      "dist/nodes/Datadog/DatadogTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
